  (DESCRIPTION =
    (ADDRESS_LIST =
      (ADDRESS = (PROTOCOL = TCP)(HOST = 192.168.0.248)(PORT = 1521))
    )
    (CONNECT_DATA =
      (SERVICE_NAME = bank)
    )
  )


ALTER SYSTEM SET LOG_ARCHIVE_CONFIG='DG_CONFIG=(bank,bankb)' scope=both;

